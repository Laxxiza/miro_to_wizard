{
    "name": "miro_to_wizard",
    "version": "v0.0.3",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "electron .",
        "dist": "electron-builder",
        "clean": "rimraf electron/output",
        "dist-clean": "npm run clean && electron-builder"
    },
    "build": {
        "appId": "com.electron.miro_to_wizard",
        "productName": "miro_to_wizard",
        "directories": {
            "output": "build/output",
            "buildResources": "build/buildResources"
        },
        "asar": false,
        "files": [
            "index.js",
            "preload.js",
            "script.js",
            "scripts/**",
            "public/**/*",
            "miro_auto_graph/**",
            "miro-export/**",
            "node_modules/**/*",
            "!node_modules/electron",
            "!node_modules/electron-builder",
            "!node_modules/.bin",
            "!**/*.md",
            "!test",
            "!tests",
            "!build",
            "!output.json",
            "!data.json"
        ],
        "win": {
            "target": [
                "portable"
            ]
        },
        "portable": {
            "artifactName": "miro_wizard_${version}.exe"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "commander": "^12.1.0",
        "he": "^1.2.0",
        "JSONStream": "^1.3.5",
        "@commander-js/extra-typings": "^12.0.0",
        "puppeteer": "^22.1.0"
    },
    "devDependencies": {
        "colors": "^1.4.0",
        "electron": "^34.2.0",
        "electron-builder": "^25.1.8",
        "rimraf": "^6.0.1"
    }
}
